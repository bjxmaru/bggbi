<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>



    <package name="jasper" namespace="/jasper" extends="default">


        <action name="jasper_compile" class="ParameterJasperAction" method="jasperCompile" >
            <result>/WEB-INF/jsp/query-form.jsp</result>
            <result name="jasperCompile" type="chain">
                <param name="actionName" >jasper_font</param>
                <param name="namespace" >/jasper</param>
                <param name="methodName" >jasperDataSourceFont</param>
            </result>
        </action>

        <action name="jasper_index" class="ParameterJasperAction" method="index" >
            <result>/WEB-INF/jsp/query-form.jsp</result>
        </action>

        <action name="jasper_dataSource" class="DataSourceJasperAction" method="jasperDataSource" >
            <result name="jasperDataSource" type="jasper">
                <param name="dataSource">customBeanList</param>
                <param name="format">HTML</param>
                <param name="reportParameters">reportParameters</param>
                <param name="location">/WEB-INF/jasperDesignJasper/testModel/dataSourceReport.jasper</param>
            </result>
        </action>

     <action name="jasper_font" class="DataSourceJasperAction" method="jasperDataSourceFont" >
            <result name="jasperDataSourceFont" type="jasper">
                <param name="dataSource">customBeanList</param>
                <param name="format">HTML</param>
                <param name="reportParameters">reportParameters</param>
                <param name="location">/WEB-INF/jasperDesignJasper/testModel/font.jasper</param>
            </result>
        </action>



    </package>




</struts>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>



    <package name="jasper" namespace="/jasper" extends="default">


        <action name="jasper_compile" class="ParameterJasperAction" method="compile" >
            <result>/WEB-INF/jsp/query-form.jsp</result>
        </action>

        <action name="jasper_index" class="ParameterJasperAction" method="index" >
            <result>/WEB-INF/jsp/query-form.jsp</result>
        </action>



    </package>




</struts>

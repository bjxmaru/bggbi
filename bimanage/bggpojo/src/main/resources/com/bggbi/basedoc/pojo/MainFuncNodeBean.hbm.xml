<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping  package="com.bggbi.basedoc.pojo">
    <class name="MainFuncNodeBean" table="mainFucTree"  optimistic-lock="version">

        <id name="id" type="integer" >
            <column name="id" sql-type="integer"></column>
            <generator class="native" />

         <!--   <generator class="native">
                <param name="sequence_name">seq_aa</param>
            </generator>-->
        </id>

        <version name="version" type="java.lang.Integer">
            <column name="version" sql-type="Integer"/>
        </version>

        <property name="dr" type="integer" column="dr"/>

        <property name="fnName" type="string" >
            <column name="fnName" sql-type="varchar(256)" />
        </property>

        <property name="fnURL" type="string" >
            <column name="fnURL" sql-type="varchar(256)" />
        </property>

        <property name="bInitOpen" type="boolean" >
            <column name="bInitOpen" sql-type="smallint" />
        </property>

        <property name="bParent"  type="boolean" >
            <column name="bParent" sql-type="smallint" />
        </property>

        <set name="childrenSet"  inverse="true" cascade="none" lazy="true" >
            <key>
                <column name="pid" sql-type="integer"/>
            </key>

            <one-to-many class="com.bggbi.basedoc.pojo.MainFuncNodeBean" ></one-to-many>
        </set>

    </class>
</hibernate-mapping>